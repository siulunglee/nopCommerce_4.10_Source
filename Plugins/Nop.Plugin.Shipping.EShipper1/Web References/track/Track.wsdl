<wsdl:definitions name="Track" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:error="http://www.EShipper1.com/XMLSchema/XOLTWS/Error/v1.1" xmlns:EShipper1s="http://www.EShipper1.com/XMLSchema/XOLTWS/EShipper1S/v1.0" xmlns:trk="http://www.EShipper1.com/XMLSchema/XOLTWS/Track/v2.0" xmlns:tns="http://www.EShipper1.com/WSDL/XOLTWS/Track/v2.0" targetNamespace="http://www.EShipper1.com/WSDL/XOLTWS/Track/v2.0">
	<wsdl:types>
		<xsd:schema>
			<!-- This schema defines the EShipper1 Security header used for authorization purposes -->
			<xsd:import namespace="http://www.EShipper1.com/XMLSchema/XOLTWS/EShipper1S/v1.0" schemaLocation="EShipper1Security.xsd"/>
			<!--  This schema defines the error detail data types returned within SOAPFaults to provide more specific information pertaining to the problem. -->
			<xsd:import namespace="http://www.EShipper1.com/XMLSchema/XOLTWS/Error/v1.1" schemaLocation="Error1.1.xsd"/>
			<!-- This schema defines the Tracking service data types -->
			<xsd:import namespace="http://www.EShipper1.com/XMLSchema/XOLTWS/Track/v2.0" schemaLocation="TrackWebServiceSchema.xsd"/>
		</xsd:schema>
	</wsdl:types>
	<wsdl:message name="TrackRequestMessage">
		<wsdl:part name="Body" element="trk:TrackRequest"/>
		<wsdl:part name="EShipper1Security" element="EShipper1s:EShipper1Security"/>
	</wsdl:message>
	<wsdl:message name="TrackResponseMessage">
		<wsdl:part name="Body" element="trk:TrackResponse"/>
	</wsdl:message>
	<wsdl:message name="TrackErrorMessage">
		<wsdl:part name="TrackError" element="error:Errors"/>
	</wsdl:message>
	<wsdl:portType name="TrackPortType">
		<wsdl:operation name="ProcessTrack">
			<wsdl:input name="TrackRequest" message="tns:TrackRequestMessage"/>
			<wsdl:output name="TrackResponse" message="tns:TrackResponseMessage"/>
			<wsdl:fault name="TrackError" message="tns:TrackErrorMessage"/>
		</wsdl:operation>
	</wsdl:portType>
	<wsdl:binding name="TrackBinding" type="tns:TrackPortType">
		<soap:binding transport="http://schemas.xmlsoap.org/soap/http"/>
		<wsdl:operation name="ProcessTrack">
			<soap:operation soapAction="http://onlinetools.EShipper1.com/webservices/TrackBinding/v2.0" style="document"/>
			<wsdl:input name="TrackRequest">
				<soap:body parts="Body" use="literal"/>
				<soap:header message="tns:TrackRequestMessage" part="EShipper1Security" use="literal">
					<soap:headerfault message="tns:TrackErrorMessage" part="TrackError" use="literal"/>
				</soap:header>
			</wsdl:input>
			<wsdl:output name="TrackResponse">
				<soap:body parts="Body" use="literal"/>
			</wsdl:output>
			<wsdl:fault name="TrackError">
				<soap:fault name="TrackError" use="literal"/>
			</wsdl:fault>
		</wsdl:operation>
	</wsdl:binding>
	<wsdl:service name="TrackService">
		<wsdl:port name="TrackPort" binding="tns:TrackBinding">
			<!-- Production URL -->
			<!-- <soap:address location="https://onlinetools.EShipper1.com/webservices/Track"/> -->
			<!-- CIE (Customer Integration Environment) URL -->
			<soap:address location="https://wwwcie.EShipper1.com/webservices/Track"/>
		</wsdl:port>
	</wsdl:service>
</wsdl:definitions>
